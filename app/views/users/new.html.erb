<form class="form-vertical">
  <h1>Signup!</h1>
  <fieldset>
<%= bootstrap_form_for(:user, url: '/users', layout: :inline, label_col: "col-sm-2", control_col: "col-sm-4") do |f| %>
    <%= f.text_field :first_name %>
    <%= f.text_field :last_name %>
    <%= f.text_field :email %>
    <%= f.password_field :password %>
    <%= f.password_field :password_confirmation %>
    <%= f.submit "Submit" %>
  <% end %>
  </fieldset>
</form>

